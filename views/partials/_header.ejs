<header>
        <nav class="navbar navbar-expand-md navbar-dark bg-Info">
          <a class="navbar-brand" href="/urls">TinyApp</a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                  <a class="nav-item nav-link" style="padding: 20px;" href="/urls">My URLs</a>
                  <a class="nav-item nav-link" style="padding: 20px;" href="/urls/new">Create New URL</a>
                  
                  <div class="form-inline" style="margin-left: 600px"> 
                      <% if (user) {%>
                        <form class="form-inline" method="POST" action="/logout">
                          <a class="nav-item nav-link inline" style="padding: 20px">Logged in as <%=user.email%></a>
                          <button type="submit" class="btn btn-outline-secondary">Logout</button>
                        </form>
                      <%} else {%>
                        <div style="float: right;">
                            <form class="form-inline" method="POST" action="/loginbtn">
                              <a class="nav-item nav-link" style="padding: 20px;" href="/register">Register</a>
                              <a class="nav-item nav-link" style="padding: 20px;" href="/login">Login</a>
                          </form>
                        </div>
                      <%}%>
                  </div>
                </div>
              </div>
            </nav>
          </header>
